<h1>Orders</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Phone</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">items</th>
        </tr>
    <thead>
    <tbody>
        <%@orders.each do |order|%>
            <tr>
                <th scope="row"><%= order.phone %></th>
                <td><%= order.name %></td>
                <td><%= order.address%></td>
                <td><%= order.items%></td>
            </tr>
        <%end%>
    </tbody>
</table>